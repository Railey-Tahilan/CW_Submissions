		System.out.println( );
		double top1 = 6.0*3.5 ;
		double top2 = 1.5*5 ;
		double bot = 55.6 - 30.2 ;
		double answ = (top1-top2)/bot ;
		System.out.println(answ) ;
		
		//This is an adaptable code that prints out a circumference and area of a circle using a radius
								
		double rad=4.0;
		double rad2=16.0;
		double pi=3.141592653589793238462643383279502884197;
						
		System.out.print("The circumference of the circle is ");
		System.out.print(2*pi*rad);
		System.out.print(" and the area of the circle is ");
		System.out.println(pi*rad2);
				
		



